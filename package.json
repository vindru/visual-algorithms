  },
  "dependencies": {
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "cross-env": "5.2.0",
    "express": "4.16.4",
    "fast-safe-stringify": "2.0.6",
    "winston": "3.2.0"
  }
}
